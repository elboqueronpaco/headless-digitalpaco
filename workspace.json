{
  "version": 1,
  "projects": {
    "backend-api-graphql": {
      "root": "apps/backend/api-graphql",
      "sourceRoot": "apps/backend/api-graphql/src",
      "projectType": "application",
      "prefix": "backend-api-graphql",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/backend/api-graphql",
            "main": "apps/backend/api-graphql/src/main.ts",
            "tsConfig": "apps/backend/api-graphql/tsconfig.app.json",
            "assets": ["apps/backend/api-graphql/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/backend/api-graphql/src/environments/environment.ts",
                  "with": "apps/backend/api-graphql/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "backend-api-graphql:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/backend/api-graphql/**/*.ts"]
          }
        }
      }
    },
    "nest-graphql-user": {
      "root": "libs/nest/graphql/user",
      "sourceRoot": "libs/nest/graphql/user/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/nest/graphql/user/**/*.ts"]
          }
        }
      }
    },
    "user": {
      "root": "libs/user",
      "sourceRoot": "libs/user/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/user/**/*.ts"]
          }
        }
      }
    },
    "database": {
      "root": "libs/database",
      "sourceRoot": "libs/database/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/database/**/*.ts"]
          }
        }
      }
    },
    "frontend-app-headless-digitalpaco": {
      "root": "apps/frontend/app-headless-digitalpaco",
      "sourceRoot": "apps/frontend/app-headless-digitalpaco",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/frontend/app-headless-digitalpaco",
            "outputPath": "dist/apps/frontend/app-headless-digitalpaco"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "options": {
            "buildTarget": "frontend-app-headless-digitalpaco:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "frontend-app-headless-digitalpaco:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "frontend-app-headless-digitalpaco:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/frontend/app-headless-digitalpaco/**/*.{ts,tsx}"
            ]
          }
        }
      }
    },
    "frontend-app-headless-digitalpaco-e2e": {
      "root": "apps/frontend/app-headless-digitalpaco-e2e",
      "sourceRoot": "apps/frontend/app-headless-digitalpaco-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/frontend/app-headless-digitalpaco-e2e/cypress.json",
            "tsConfig": "apps/frontend/app-headless-digitalpaco-e2e/tsconfig.e2e.json",
            "devServerTarget": "frontend-app-headless-digitalpaco:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "frontend-app-headless-digitalpaco:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/frontend/app-headless-digitalpaco-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "backend-api-graphql",
  "schematics": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  }
}
